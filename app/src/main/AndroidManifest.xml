<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ppandroid.app">

    <!-- 获取运营商信息，用于支持提供运营商信息相关的接口 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <!-- 用于访问网络，网络定位需要上网 -->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <!-- 写入扩展存储，向扩展卡写入数据，用于写入缓存定位数据 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE">
    </uses-permission>

    <application
        android:name="com.ppandroid.im.APP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AC_Main">
        </activity>
        <activity android:name=".base.AC_ContentFG"/>
        <activity android:name="com.ppandroid.im.base.AC_Base"/>
        <activity android:name=".AC_Login">
        </activity>
        <activity android:name=".AC_Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".demo.AC_ADemo">
        </activity>
        <activity android:name=".demo.AC_BDemo">
        </activity>

        <service android:name=".utils.upgrade.NotificationService"/>

        <activity
            android:screenOrientation="landscape"
            android:name=".demo.AC_HorChart">
        </activity>
    </application>

</manifest>